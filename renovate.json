{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>gwennlbh/renovate"],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "enabled": true
    },
    {
      "matchDepTypes": ["requires-python"],
      "enabled": false
    },
    {
      "matchPackageNames": ["vscode"],
      "enabled": true
    }
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  }
}
